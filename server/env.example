# LiveKit Configuration
LIVEKIT_URL=ws://localhost:7880
LIVEKIT_API_KEY=devkey
LIVEKIT_API_SECRET=secret

# Deepgram Configuration (for speech-to-text and text-to-speech)
# Get your API key from: https://console.deepgram.com/
DEEPGRAM_API_KEY=your_deepgram_api_key_here

# OpenRouter Configuration (for LLM)
# Get your API key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Cartesia Configuration (for text-to-speech)
# Get your API key from: https://cartesia.ai/
CARTESIA_API_KEY=your_cartesia_api_key_here

# OpenAI Configuration (fallback)
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Database Configuration (optional)
DATABASE_URL=postgresql://localhost:5432/confidence_compass

# Server Configuration
PORT=5000
NODE_ENV=development 